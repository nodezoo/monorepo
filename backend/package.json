{
  "name": "@nodezoo/backend",
  "version": "0.0.1",
  "description": "Nodezoom Backend",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "test-some": "jest -t",
    "test-watch": "jest --coverage --watchAll",
    "test-view": "open coverage/lcov-report/index.html",
    "model": "voxgig-model model/model.jsonic",
    "model-build": "voxgig-model model/model.jsonic --once",
    "repl": "seneca-repl localhost:50302",
    "watch": "tsc --build src -w",
    "build": "npm run model-build && tsc --build src",
    "clean": "rm -rf dist node_modules yarn.lock package-lock.json",
    "reset": "npm run clean && npm install && npm run build && npm test",
    "prep-dev": "npm link @nodezoo/nodezoo-common",
    "prep-vxg": "npm link @nodezoo/nodezoo-common @voxgig/model @voxgig/build @voxgig/system",
    "local": "node dist/env/local/local.js --seneca.log.warn",
    "local-debug": "node dist/env/local/local.js --seneca.log.debug",
    "repo-tag": "REPO_VERSION=`node -e \"console.log(require('./package').version)\"` && echo TAG: nzo-backend-$REPO_VERSION && git -u . && git commit -m v$REPO_VERSION && git push && git tag v$REPO_VERSION && git push --tags"
  },
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@nodezoo/common": "^0.0.2",
    "@seneca/capture": "^0.5.0",
    "@seneca/dynamo-store": "4.3.1",
    "@seneca/entity-util": "1.6.0",
    "@seneca/env": "0.2.0",
    "@seneca/gateway": "1.0.0",
    "@seneca/gateway-auth": "1.0.2",
    "@seneca/gateway-lambda": "0.9.0",
    "@seneca/owner": "^6.1.0",
    "@seneca/reload": "0.4.1",
    "@seneca/repl": "7.0.1",
    "@seneca/s3-store": "^1.0.0",
    "@seneca/user": "6.3.0",
    "@voxgig/model": "1.0.0",
    "@voxgig/system": "0.8.0",
    "argparse": "2.0.1",
    "axios": "1.6.3",
    "seneca": "3.33.0",
    "seneca-entity": "25.1.1",
    "seneca-promisify": "3.7.1",
    "uuid": "9.0.1"
  },
  "devDependencies": {
    "@seneca/gateway-express": "0.10.0",
    "@tsconfig/node16": "16.1.1",
    "@types/express": "^4.17.21",
    "@types/jest": "29.5.11",
    "@types/uuid": "^9.0.7",
    "@voxgig/build": "1.1.1",
    "aws-sdk": "^2.1528.0",
    "cookie-parser": "1.4.6",
    "es-jest": "2.1.0",
    "esbuild": "0.19.11",
    "express": "4.18.2",
    "jest": "29.7.0",
    "prettier": "3.1.1",
    "typescript": "5.3.3"
  }
}
